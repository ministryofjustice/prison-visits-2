<h2 class="heading-medium">Statistics for integration outcomes</h2>
<%= render 'metrics/content_for_head' %>
<%= content_tag :div, '', { class: 'js-Metrics',              data: { percentiles: @metrics_presenter.percentiles_by_day } } %>
<%= content_tag :div, '', { class: 'js-VisitsCounts',         data: { visit_counts: @metrics_presenter.visits_per_processing_state } } %>
<%= content_tag :div, '', { class: 'js-TimelyVisitsCount',    data: { visit_counts: @metrics_presenter.timely_and_overdue } } %>
<%= content_tag :div, '', { class: 'js-RejectionPercentages', data: { rejections_percentages: @metrics_presenter.rejection_percentages } } %>

<hr/>
